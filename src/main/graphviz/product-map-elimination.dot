digraph {
    Start[class="extremity"]
    Goal[class="extremity"]

    FA[label="F[A]" group="upper" class="start"]
    FB[label="F[B]" group="upper" class="goal,disabled"]

    A[group="lower"]
    B[group="lower" class="disabled"]
    X[group="upper"]

    {
        rank=same
        FB -> B[dir="back" style="invis"]
    }

        {
        rank=same
        FA -> A
    }


    FA -> X

    A -> B[label="f" class="disabled"]
    X -> FB[style="invis"]


    Start -> FA[class="start"]
    FB -> Goal[class="goal,disabled"]
}
