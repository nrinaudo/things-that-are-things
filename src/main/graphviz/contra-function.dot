digraph {
    X[group="upper"]
    X2[group="lower" style="invis"]

    A[group="upper"]
    B[group="lower"]

    {
      rank=same
       X -> X2[style="invis"]
    }


    {
      rank=same
       A -> B[label="f"]
    }

    A -> X[label="F[A]" class="start"]
    B -> X[label="F[B]" style="invis"]
    X2 -> B[style="invis"]
}
