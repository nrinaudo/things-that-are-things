digraph {
    Start[class="extremity"]
    Goal[class="extremity"]

    AB[label="A×B"]
    C
    FAFB[label="F[A]×F[B]" class="start"]
    FC[label="F[C]" class="goal"]

    FAFB -> FC[label="map2(f)"]
    AB -> C[label="f"]

    {
        rank=same
        FAFB -> AB[dir=back class="lift"]
    }

    {
        rank=same
        FC -> C[dir=back class="lift"]
    }

    Start -> FAFB[class="start"]
    FC -> Goal[class="goal"]
}
